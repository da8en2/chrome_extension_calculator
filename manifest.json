{
  "manifest_version": 2,
  "name": "Calculator",
  "version": "1.0",
  "description": "A simple Calculator by Daniel Endres",
  "icons": {
    "128": "calc128.png",
    "48": "calc48.png",
    "16": "calc16.png"
  },
  "browser_action": {
    "default_icon": "calc16.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "storage"
  ]
}
